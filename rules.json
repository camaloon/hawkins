{
  "rules": {
    ".read": true,
    ".write": true,
    "pushes": {
      "$item": {
        ".validate": "newData.hasChildren() && newData.child('pusher').exists() && newData.child('deleted').val() === false && !(/\[ci skip\]|\[skip ci\]/i).test(newData.child('head_commit').child('message').val())"
      }
    }
  }
}
